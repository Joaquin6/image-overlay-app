{
  "name": "image-overlay-app",
  "version": "0.0.1",
  "description": "Single page that can run in the browser that outputs canvas data.",
  "main": "./bin/www",
  "author": "Joaquin Briceno <joaquinbriceno1@gmail.com>",
  "license": "ISC",
  "homepage": "https://github.com/Joaquin6/image-overlay-app#readme",
  "config": {
    "port": "8080",
    "path": "./"
  },
  "engines": {
    "node": ">=6.2.1",
    "npm": "~2.15.1"
  },
  "scripts": {
    "env": "env",
    "inspector": "node-inspector",
    "autoprefixer": "postcss -u --local-plugins -u autoprefixer -m file -r ./app/css/styles.css",
    "sass": "node-sass --output-style nested --indent-type tab --indent-width 1 ./app/sass/styles.scss ./app/css/styles.css",
    "sass:watch": "npm run sass -- -w ./app/sass",
    "server": "node $npm_package_main",
    "server-debug": "node --debug $npm_package_main",
    "server-debug-brk": "node --debug-brk $npm_package_main",
    "start": "NODE_PATH=$npm_package_config_path npm run server",
    "start-debug": "NODE_PATH=$npm_package_config_path npm run server-debug",
    "start-debug-brk": "NODE_PATH=$npm_package_config_path npm run server-debug-brk",
    "dev": "webpack --watch & NODE_PATH=$npm_package_config_path npm run server",
    "build": "webpack",
    "postinstall": "npm install webpack imagemin node-sass postcss -g",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "aws-sdk": "^2.7.0",
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.3.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "body-parser": "~1.13.2",
    "clean-webpack-plugin": "^0.1.15",
    "cookie-parser": "~1.3.5",
    "css-loader": "^0.26.2",
    "debug": "~2.2.0",
    "express": "~4.13.1",
    "express-debug": "^1.1.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.10.1",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.28.0",
    "html-webpack-pug-plugin": "0.0.3",
    "http-proxy": "^1.16.2",
    "imagemin": "^5.2.2",
    "imagemin-gifsicle": "^5.1.0",
    "imagemin-jpegtran": "^5.0.2",
    "imagemin-pngquant": "^5.0.0",
    "imagemin-svgo": "^5.2.0",
    "jquery": "^1.12.4",
    "jquery-ui": "^1.12.1",
    "js-cookie": "^2.1.3",
    "json-loader": "^0.5.4",
    "markdown-loader": "^2.0.0",
    "node-sass": "^4.5.0",
    "on-build-webpack": "^0.1.0",
    "postcss": "^5.2.15",
    "postcss-loader": "^1.3.3",
    "pug": "^2.0.0-beta11",
    "pug-html-loader": "^1.1.1",
    "raw-loader": "^0.5.1",
    "requirejs": "^2.3.2",
    "requirejs-text": "^2.0.15",
    "sass-loader": "^6.0.2",
    "serve-favicon": "~2.3.0",
    "shortid": "^2.2.6",
    "stats-webpack-plugin": "^0.4.3",
    "style-loader": "^0.13.2",
    "superagent": "^3.5.0",
    "svg-url-loader": "^2.0.2",
    "url-loader": "^0.5.8",
    "webpack": "^2.2.1",
    "webpack-error-notification": "^0.1.6"
  },
  "devDependencies": {
    "node-debug": "^0.1.0",
    "node-inspector": "^0.12.8",
    "webpack-dev-middleware": "^1.10.0",
    "webpack-dev-server": "^2.3.0",
    "webpack-hot-middleware": "^2.16.1",
    "on-build-webpack": "^0.1.0"
  }
}
