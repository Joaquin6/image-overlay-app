{
  "name": "image-overlay-app",
  "version": "0.0.1",
  "description": "Single page that can run in the browser that outputs canvas data.",
  "main": "./server/app.js",
  "author": "Joaquin Briceno <joaquinbriceno1@gmail.com>",
  "license": "ISC",
  "homepage": "https://github.com/Joaquin6/image-overlay-app#readme",
  "config": {
    "port": "8080",
    "path": "./"
  },
  "engines": {
    "node": ">=6.2.1",
    "npm": "~2.15.1"
  },
  "scripts": {
    "env": "env",
    "inspector": "node-inspector",
    "autoprefixer": "postcss -u --local-plugins -u autoprefixer -m file -r ./app/css/styles.css",
    "sass": "node-sass --output-style nested --indent-type tab --indent-width 1 app/sass/styles.scss app/css/styles.css",
    "postsass": "npm run autoprefixer",
    "sass:watch": "npm run sass -- -w ./app/sass",
    "server": "node $npm_package_main",
    "server-debug": "node --debug $npm_package_main",
    "server-debug-brk": "node --debug-brk $npm_package_main",
    "start": "npm run server",
    "start-debug": "NODE_PATH=$npm_package_config_path npm run server-debug",
    "start-debug-brk": "NODE_PATH=$npm_package_config_path npm run server-debug-brk",
    "build": "webpack index=./app/js/app.js --output-path='./release' -p --progress --colors",
    "postinstall": "npm install webpack imagemin node-sass postcss autoprefixer -g",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "body-parser": "~1.13.2",
    "compression": "^1.6.2",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "express": "~4.13.1",
    "express-debug": "^1.1.1",
    "http-proxy": "^1.16.2",
    "imports-loader": "^0.7.1",
    "jquery": "^1.12.4",
    "jquery-ui": "^1.12.1",
    "js-cookie": "^2.1.3",
    "json-loader": "^0.5.4",
    "markdown-loader": "^2.0.0",
    "method-override": "^2.3.7",
    "node-sass": "^4.5.0",
    "on-build-webpack": "^0.1.0",
    "postcss": "^5.2.15",
    "postcss-cli": "^3.0.0-beta",
    "postcss-loader": "^1.3.3",
    "pug": "^2.0.0-beta11",
    "pug-html-loader": "^1.1.1",
    "raw-loader": "^0.5.1",
    "requirejs": "^2.3.2",
    "requirejs-text": "^2.0.15",
    "resolve-url-loader": "^2.0.2",
    "sass-loader": "^6.0.2",
    "serve-favicon": "~2.3.0",
    "shortid": "^2.2.6"
  },
  "devDependencies": {
    "compression-webpack-plugin": "^0.3.2",
    "node-debug": "^0.1.0",
    "node-inspector": "^0.12.8",
    "uglifyjs-webpack-plugin": "^0.3.0",
    "url-loader": "^0.5.8"
  }
}
